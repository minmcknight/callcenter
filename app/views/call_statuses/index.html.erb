<p id="notice"><%= notice %></p>

<h1>Listing Call Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @call_statuses.each do |call_status| %>
      <tr>
        <td><%= call_status.name %></td>
        <td><%= link_to 'Show', call_status %></td>
        <td><%= link_to 'Edit', edit_call_status_path(call_status) %></td>
        <td><%= link_to 'Destroy', call_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Call status', new_call_status_path %>
